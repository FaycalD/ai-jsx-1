{
  "extends": "@tsconfig/node18/tsconfig.json",
  "include": ["src/**/*", ".eslintrc.cjs"],
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "AI.createElement",
    "jsxFragmentFactory": "AI.Fragment",
    "moduleResolution": "node16",
    "module": "esnext",
    "resolveJsonModule": true,
    "outDir": "dist",
    "declaration": true
  }
}
